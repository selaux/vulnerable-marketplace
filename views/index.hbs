<h1>{{ pageTitle }}</h1>
<h2>Products</h2>
<form action="/" method="GET">
    <label>Price</label>
    <label>From</label><input type="number" value="{{ minPrice }}" name="min">
    <label>To</label><input type="number" value="{{ maxPrice }}" name="max">
    <input type="submit" value="apply">
</form>
<ul>
    {{#each products}}
        <li style="height: 160px; margin-bottom: 20px; list-style-type: none;">
            <img style="float: left; padding-right: 10px;" width="100" height="160" alt="star" src="data:image/jpg;base64,{{ image }}">
            <p>{{ name }} - {{ price }}</p>
            <p>{{ description }}</p>
        </li>
    {{/each}}
</ul>